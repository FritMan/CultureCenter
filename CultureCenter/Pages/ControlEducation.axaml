<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="CultureCenter.Pages.ControlEducation">
	<Grid ColumnDefinitions="*" RowDefinitions="50, *, 90">

		<StackPanel Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical">
      <Label Name="Types" Content="Образование" HorizontalAlignment="Center">
      </Label>
      <Button Name="TeachersBtn" Content="Преподаватели" Margin="5" Width="150" HorizontalContentAlignment="Center"/>
      <Button Name="MugsBtn" Content="Кружки" Margin="5" Width="150" HorizontalContentAlignment="Center"/>
      <Button Name="MugsTypesBtn" Content="Виды кружков" Margin="5" Width="150" HorizontalContentAlignment="Center"/>
      <Button Name="EnlightenmentBtn" Content="Помещения" Margin="5" Width="150" HorizontalContentAlignment="Center"/>
      <Button Name="DaysBtn" Content="Дни недели" Margin="5" Width="150" HorizontalContentAlignment="Center"/>
			<Button Name="BackBtn" Content="На главное меню" Margin="5" Width="150" HorizontalContentAlignment="Center"/>
		</StackPanel>
		
	</Grid>
</UserControl>
