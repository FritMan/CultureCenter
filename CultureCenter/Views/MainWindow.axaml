<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:CultureCenter.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:CultureCenter.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="CultureCenter.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="CultureCenter" MinWidth="800" MinHeight="600">
	<Grid ColumnDefinitions="100, *, 100" RowDefinitions="50, *">
		<Button Name="ExitBtn" Content ="Выход" Background="Red"
				Grid.Row="0" Grid.Column="2"/>

		<ContentControl Grid.Row="2" Grid.ColumnSpan="3" Name="MainFrame" />
	</Grid>
</Window>
